
    let listDeletes = [
        "/user/apoelaras/",
    ];


    function delBoxes() {

        for (let i = 0; i < listDeletes.length; i++) {
            document.querySelectorAll(`div a[href^="${listDeletes[i]}"]`).forEach(e => e.closest('[data-testid="post-container"]').parentNode.parentNode.remove());
        }
    }
    delBoxes();


    const observer1 = new MutationObserver(function (mutation) {
        mutation.forEach(function (mutation) {
            if (mutation.addedNodes.length) {
                delBoxes();
            }
        })
    });


    if (document.querySelector('._1OVBBWLtHoSPfGCRaPzpTf') != null) {
        const playlistWrapper1 = document.querySelector('._1OVBBWLtHoSPfGCRaPzpTf');
        observer1.observe(playlistWrapper1, {
            childList: true,
            attributes: true,
            subtree: true,
            characterData: true,
        })
    }











